{
    "name": "bzimor/yii2-autopost",
    "description": "This extension allows you to post messages, photos or videos on famous Social networks such as Telegram, Facebook and Twitter",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","telegram","facebook","twitter","autopost"],
    "license": "MIT",
    "authors": [
        {
            "name": "Boburmirzo Hamraqulov",
            "email": "bobzimor@gmail.com",
            "homepage": "https://github.com/bzimor/yii2-autopost",
            "role": "Developer"
        }
    ],
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/bzimor/yii2-autopost"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "jublonet/codebird-php":"*",
        "facebook/graph-sdk":"*",
        "aki/yii2-bot-telegram":"*"

    },
    "extra": {
        "bootstrap": "bzimor\\autopost\\Bootstrap"
    },
    "autoload": {
        "psr-4": {
            "bzimor\\autopost\\": "src"
        }
    }
}
